{
  "name": "rerepa-starter-kit",
  "version": "1.0.0",
  "description": "Yet another minimal frontend starter kit with React.js and Parcel Bundler",
  "author": "Hoang Nhat Nguyen <hoangnn93@gmail.com>",
  "license": "MIT",
  "keywords": [
    "reactjs",
    "redux",
    "react-router",
    "parcel-bundler"
  ],
  "scripts": {
    "build": "NODE_ENV=production parcel build src/index.html -d build --detailed-report --no-source-maps",
    "clean": "rimraf build dist",
    "start": "parcel ./src/index.html",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "*.js": ["eslint src"]
  },
  "dependencies": {
    "history": "^4.7.2",
    "prop-types": "^15.6.1",
    "react": ">=16.0.0",
    "react-dom": ">=16.0.0",
    "react-redux": "^5.0.7",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-router-redux": "^5.0.0-alpha.9",
    "redux": "^4.0.0",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.3",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-env": "^1.7.0",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.9.1",
    "husky": "^0.14.3",
    "lint-staged": "^7.2.0",
    "parcel-bundler": "^1.9.0",
    "react-hot-loader": "^4.3.3",
    "rimraf": "^2.6.2"
  }
}
